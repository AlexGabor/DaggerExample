<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alexgabor.daggerexample">

    <application
        android:allowBackup="false"
        android:name="com.alexgabor.daggerexample.DaggerExampleApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.alexgabor.daggerexample.feature.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.alexgabor.daggerexample.feature.injectedViewModelInActivity.Example1Activity" />
        <activity android:name="com.alexgabor.daggerexample.feature.injectedViewModelInFragment.Example2Activity" />
        <activity android:name="com.alexgabor.daggerexample.feature.injectedSharedViewModel.Example3Activity" />
        <activity android:name="com.alexgabor.daggerexample.feature.assistedInject.Example4Activity" />
        <activity android:name="com.alexgabor.daggerexample.feature.viewmodelscope.Example5Activity" />
    </application>

</manifest>